isbn = {1537-5110},
issn = {15375110},
journal = {Biosystems Engineering},
number = {2},
pages = {127--137},
title = {{Organic farming scenarios: Operational analysis and costs of implementing innovative technologies}},
volume = {91},
year = {2005}
}
