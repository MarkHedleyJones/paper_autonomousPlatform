journal = {Computers and Electronics in Agriculture},
keywords = {Fruit harvesting,Image segmentation,Machine vision,Random ring method,Stereo matching},
pages = {68--74},
publisher = {Elsevier B.V.},
title = {{Location of apples in trees using stereoscopic vision}},
url = {http://dx.doi.org/10.1016/j.compag.2015.01.010},
volume = {112},
year = {2015}
}
