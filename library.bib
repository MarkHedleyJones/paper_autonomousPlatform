isbn = {9789086861132},
journal = {Proceedings of Joint International Agricultural Conference (2009)},
pages = {841--847},
title = {{BoniRob: an autonomous field robot platform for individual plant phenotyping}},
url = {https://my.hs-osnabrueck.de/ecs/fileadmin/groups/156/Veroeffentlichungen/2009-JIAC-BoniRob.pdf},
volume = {9},
year = {2009}
}
