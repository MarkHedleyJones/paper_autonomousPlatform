keywords = {genetic algorithm,mobile robot,neural network,path planning},
number = {2-3},
pages = {187--204},
title = {{Path planning of an agricultural mobile robot by neural network and genetic algorithm}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S016816999700029X},
